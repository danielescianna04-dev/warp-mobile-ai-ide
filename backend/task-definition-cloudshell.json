{
    "family": "warp-flutter-web-task",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::703686967361:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::703686967361:role/ECSTaskRole",
    "runtimePlatform": {
        "cpuArchitecture": "ARM64",
        "operatingSystemFamily": "LINUX"
    },
    "containerDefinitions": [
        {
            "name": "warp-flutter-container",
            "image": "703686967361.dkr.ecr.us-west-2.amazonaws.com/warp-ecs-flutter:cloudshell",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/warp-flutter-web-task",
                    "awslogs-region": "us-west-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "environment": [
                {
                    "name": "PORT",
                    "value": "3000"
                }
            ]
        }
    ]
}