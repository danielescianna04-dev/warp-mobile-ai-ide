{
    "family": "warp-mobile-ai-ide-prod-task",
    "taskRoleArn": "arn:aws:iam::703686967361:role/warp-mobile-ai-ide-prod-ecs-task-role",
    "executionRoleArn": "arn:aws:iam::703686967361:role/warp-mobile-ai-ide-prod-ecs-execution-role",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
        {
            "name": "warp-mobile-ai-ide-container",
            "image": "703686967361.dkr.ecr.us-east-1.amazonaws.com/warp-mobile-ai-ide:13-amd64",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "PORT",
                    "value": "3000"
                },
                {
                    "name": "NODE_ENV",
                    "value": "production"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/warp-mobile-ai-ide-prod",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:3000/health || exit 1"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 3,
                "startPeriod": 60
            }
        }
    ]
}