{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "ecs:DescribeServices",
                "ecs:UpdateService",
                "ecs:DescribeTasks",
                "ecs:ListTasks"
            ],
            "Resource": [
                "arn:aws:ecs:us-east-1:703686967361:cluster/warp-mobile-ai-ide-prod-cluster",
                "arn:aws:ecs:us-east-1:703686967361:service/warp-mobile-ai-ide-prod-cluster/warp-mobile-ai-ide-prod-ecs-service",
                "arn:aws:ecs:us-east-1:703686967361:task/warp-mobile-ai-ide-prod-cluster/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "dynamodb:GetItem",
                "dynamodb:PutItem",
                "dynamodb:UpdateItem",
                "dynamodb:DeleteItem",
                "dynamodb:Query",
                "dynamodb:Scan"
            ],
            "Resource": [
                "arn:aws:dynamodb:us-east-1:703686967361:table/warp-mobile-ai-ide-prod-sessions"
            ]
        }
    ]
}