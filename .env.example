# ‚öôÔ∏è Configurazioni ambiente
ENVIRONMENT=development
DEBUG_MODE=true
LOG_LEVEL=debug

# üöÄ Agent Mode Configuration (opzionale)
AGENT_MODE_ENABLED=true
AGENT_MAX_ITERATIONS=15
AGENT_TIMEOUT_SECONDS=600
AGENT_AUTO_APPROVE=false
DEBUG_AI=true